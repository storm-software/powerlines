{
  "name": "@powerlines/plugin-eslint",
  "version": "0.8.285",
  "type": "module",
  "description": "A package containing a Powerlines plugin for running ESLint on the codebase.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/powerlines.git",
    "directory": "packages/plugin-eslint"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": {
    "url": "https://stormsoftware.com/support",
    "email": "support@stormsoftware.com"
  },
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "maintainers": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "contributors": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Apache-2.0",
  "private": false,
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./helpers": {
      "require": {
        "types": "./dist/helpers/index.d.cts",
        "default": "./dist/helpers/index.cjs"
      },
      "import": {
        "types": "./dist/helpers/index.d.mts",
        "default": "./dist/helpers/index.mjs"
      },
      "default": {
        "types": "./dist/helpers/index.d.mts",
        "default": "./dist/helpers/index.mjs"
      }
    },
    "./helpers/format-message": {
      "require": {
        "types": "./dist/helpers/format-message.d.cts",
        "default": "./dist/helpers/format-message.cjs"
      },
      "import": {
        "types": "./dist/helpers/format-message.d.mts",
        "default": "./dist/helpers/format-message.mjs"
      },
      "default": {
        "types": "./dist/helpers/format-message.d.mts",
        "default": "./dist/helpers/format-message.mjs"
      }
    },
    "./package.json": "./package.json",
    "./types": {
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/types/index.cjs"
      },
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/types/index.mjs"
      },
      "default": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/types/index.mjs"
      }
    },
    "./types/plugin": {
      "require": {
        "types": "./dist/types/plugin.d.cts",
        "default": "./dist/types/plugin.cjs"
      },
      "import": {
        "types": "./dist/types/plugin.d.mts",
        "default": "./dist/types/plugin.mjs"
      },
      "default": {
        "types": "./dist/types/plugin.d.mts",
        "default": "./dist/types/plugin.mjs"
      }
    }
  },
  "files": ["dist/**/*"],
  "keywords": ["eslint", "powerlines", "storm-software", "powerlines-plugin"],
  "dependencies": {
    "@storm-software/config-tools": "catalog:",
    "@stryke/fs": "catalog:",
    "@stryke/path": "catalog:",
    "defu": "catalog:",
    "eslint": "catalog:",
    "powerlines": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts"
}
