{
  "name": "@powerlines/plugin-prisma",
  "version": "0.2.180",
  "type": "module",
  "description": "A Powerlines plugin to generate project code and a Prisma client from a Prisma schema (PSL).",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/powerlines.git",
    "directory": "packages/plugin-prisma"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": {
    "url": "https://stormsoftware.com/support",
    "email": "support@stormsoftware.com"
  },
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "maintainers": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "contributors": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Apache-2.0",
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "typings": "dist/index.d.mts",
  "files": ["dist/**/*"],
  "keywords": ["prisma", "powerlines", "storm-software", "powerlines-plugin"],
  "dependencies": {
    "@prisma/get-platform": "catalog:",
    "@prisma/prisma-schema-wasm": "catalog:",
    "@stryke/cli": "catalog:",
    "@stryke/fs": "catalog:",
    "@stryke/path": "catalog:",
    "@stryke/type-checks": "catalog:",
    "@stryke/types": "catalog:",
    "defu": "catalog:",
    "fp-ts": "catalog:",
    "jiti": "catalog:",
    "powerlines": "workspace:*",
    "prisma-util": "catalog:",
    "ts-pattern": "catalog:"
  },
  "devDependencies": {
    "@powerlines/plugin-hey-api": "workspace:*",
    "@powerlines/plugin-plugin": "workspace:*",
    "@types/node": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
