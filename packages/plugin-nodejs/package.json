{
  "name": "@powerlines/plugin-nodejs",
  "version": "0.1.44",
  "type": "module",
  "description": "A package containing a Powerlines plugin for building a Node.js application.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/powerlines.git",
    "directory": "packages/plugin-nodejs"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": {
    "url": "https://stormsoftware.com/support",
    "email": "support@stormsoftware.com"
  },
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "maintainers": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "contributors": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Apache-2.0",
  "private": false,
  "typings": "dist/index.d.mts",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/types/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/types/index.cjs"
      },
      "default": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/types/index.mjs"
      }
    },
    "./types/*": {
      "import": {
        "types": "./dist/types/*.d.mts",
        "default": "./dist/types/*.mjs"
      },
      "require": {
        "types": "./dist/types/*.d.cts",
        "default": "./dist/types/*.cjs"
      },
      "default": {
        "types": "./dist/types/*.d.mts",
        "default": "./dist/types/*.mjs"
      }
    },
    "./components": {
      "import": {
        "types": "./dist/components/index.d.mts",
        "default": "./dist/components/index.mjs"
      },
      "require": {
        "types": "./dist/components/index.d.cts",
        "default": "./dist/components/index.cjs"
      },
      "default": {
        "types": "./dist/components/index.d.mts",
        "default": "./dist/components/index.mjs"
      }
    },
    "./components/*": {
      "import": {
        "types": "./dist/components/*.d.mts",
        "default": "./dist/components/*.mjs"
      },
      "require": {
        "types": "./dist/components/*.d.cts",
        "default": "./dist/components/*.cjs"
      },
      "default": {
        "types": "./dist/components/*.d.mts",
        "default": "./dist/components/*.mjs"
      }
    }
  },
  "files": ["dist/**/*"],
  "keywords": ["nodejs", "powerlines", "storm-software", "powerlines-plugin"],
  "dependencies": {
    "@alloy-js/core": "catalog:",
    "@alloy-js/typescript": "catalog:",
    "@powerlines/plugin-alloy": "workspace:*",
    "@powerlines/plugin-babel": "workspace:*",
    "@powerlines/plugin-env": "workspace:*",
    "@storm-software/config-tools": "catalog:",
    "@stryke/string-format": "catalog:",
    "defu": "catalog:",
    "powerlines": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "catalog:",
    "@powerlines/plugin-plugin": "workspace:*",
    "@types/node": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
