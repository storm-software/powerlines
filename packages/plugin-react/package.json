    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "require": "./dist/components/index.cjs",
      "import": "./dist/components/index.mjs"
    },
    "./components/react-optimized": {
      "require": "./dist/components/react-optimized.cjs",
      "import": "./dist/components/react-optimized.mjs"
    },
    "./types": {
      "require": "./dist/types/index.cjs",
      "import": "./dist/types/index.mjs"
    },
    "./types/plugin": {
      "require": "./dist/types/plugin.cjs",
      "import": "./dist/types/plugin.mjs"
    },
    "./types/runtime": {
      "require": "./dist/types/runtime.cjs",
      "import": "./dist/types/runtime.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist/**/*"],
  "keywords": ["react", "powerlines", "storm-software", "powerlines-plugin"],
  "peerDependencies": {
    "react": ">=19.2.0",
    "react-dom": ">=19.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "dependencies": {
    "@alloy-js/core": "catalog:",
    "@alloy-js/json": "catalog:",
    "@alloy-js/markdown": "catalog:",
    "@alloy-js/typescript": "catalog:",
    "@babel/core": "catalog:",
    "@powerlines/plugin-alloy": "workspace:*",
    "@powerlines/plugin-babel": "workspace:*",
    "@powerlines/plugin-env": "workspace:*",
    "@storm-software/config-tools": "catalog:",
    "@stryke/cli": "catalog:",
    "@stryke/convert": "catalog:",
    "@stryke/fs": "catalog:",
    "@stryke/path": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "babel-plugin-react-compiler": "catalog:",
    "defu": "catalog:",
    "powerlines": "workspace:*"
  },
  "devDependencies": {
    "@powerlines/plugin-plugin": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/index.d.cts"
