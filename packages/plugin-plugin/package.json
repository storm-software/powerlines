{
  "name": "@powerlines/plugin-plugin",
  "version": "0.12.27",
  "type": "module",
  "description": "A package containing a Powerlines plugin to assist in developing other Powerlines plugins.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/powerlines.git",
    "directory": "packages/plugin-plugin"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": {
    "url": "https://stormsoftware.com/support",
    "email": "support@stormsoftware.com"
  },
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "maintainers": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "contributors": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Apache-2.0",
  "private": false,
  "main": "dist/src/index.cjs",
  "module": "dist/src/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.js"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.cjs"
      },
      "default": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.js"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.js"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.cjs"
      },
      "default": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.js"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/src/plugin.d.ts",
        "default": "./dist/src/plugin.js"
      },
      "require": {
        "types": "./dist/src/plugin.d.ts",
        "default": "./dist/src/plugin.cjs"
      },
      "default": {
        "types": "./dist/src/plugin.d.ts",
        "default": "./dist/src/plugin.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/src/types/index.d.ts",
        "default": "./dist/src/types/index.js"
      },
      "require": {
        "types": "./dist/src/types/index.d.ts",
        "default": "./dist/src/types/index.cjs"
      },
      "default": {
        "types": "./dist/src/types/index.d.ts",
        "default": "./dist/src/types/index.js"
      }
    },
    "./types/*": {
      "import": {
        "types": "./dist/src/types/*.d.ts",
        "default": "./dist/src/types/*.js"
      },
      "require": {
        "types": "./dist/src/types/*.d.ts",
        "default": "./dist/src/types/*.cjs"
      },
      "default": {
        "types": "./dist/src/types/*.d.ts",
        "default": "./dist/src/types/*.js"
      }
    }
  },
  "typings": "dist/src/index.d.ts",
  "files": ["dist/**/*"],
  "keywords": ["powerlines", "storm-software", "powerlines-plugin"],
  "dependencies": {
    "@alloy-js/rollup-plugin": "catalog:",
    "@alloy-js/core": "catalog:",
    "@alloy-js/json": "catalog:",
    "@alloy-js/markdown": "catalog:",
    "@alloy-js/typescript": "catalog:",
    "tsdown": "catalog:",
    "unplugin": "catalog:",
    "@stryke/fs": "catalog:",
    "@stryke/json": "catalog:",
    "@stryke/path": "catalog:",
    "@stryke/string-format": "catalog:",
    "@stryke/type-checks": "catalog:",
    "@stryke/types": "catalog:",
    "@stryke/convert": "catalog:",
    "defu": "catalog:",
    "jiti": "catalog:",
    "powerlines": "workspace:*"
  },
  "devDependencies": {
    "@powerlines/nx": "workspace:*",
    "@rollup/plugin-commonjs": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@types/babel__helper-plugin-utils": "catalog:",
    "@types/node": "catalog:",
    "rollup": "catalog:",
    "rollup-plugin-typescript2": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
