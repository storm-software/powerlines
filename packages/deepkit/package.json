{
  "name": "@powerlines/deepkit",
  "version": "0.2.2",
  "type": "module",
  "description": "A package containing a Powerlines plugin to assist in developing other Powerlines plugins.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/powerlines.git",
    "directory": "packages/deepkit"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": {
    "url": "https://stormsoftware.com/support",
    "email": "support@stormsoftware.com"
  },
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "maintainers": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "contributors": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Apache-2.0",
  "private": false,
  "bin": {
    "deepkit-install": "bin/deepkit-install.cjs"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      },
      "default": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    },
    "./vendor/*": {
      "import": {
        "types": "./dist/vendor/*.d.ts",
        "default": "./dist/vendor/*.js"
      },
      "require": {
        "types": "./dist/vendor/*.d.cts",
        "default": "./dist/vendor/*.cjs"
      },
      "default": {
        "types": "./dist/vendor/*.d.ts",
        "default": "./dist/vendor/*.js"
      }
    },
    "./vendor/type-compiler": {
      "import": {
        "types": "./dist/vendor/type-compiler/index.d.ts",
        "default": "./dist/vendor/type-compiler/index.js"
      },
      "require": {
        "types": "./dist/vendor/type-compiler/index.d.cts",
        "default": "./dist/vendor/type-compiler/index.cjs"
      },
      "default": {
        "types": "./dist/vendor/type-compiler/index.d.ts",
        "default": "./dist/vendor/type-compiler/index.js"
      }
    },
    "./vendor/type-compiler/*": {
      "import": {
        "types": "./dist/vendor/type-compiler/*.d.ts",
        "default": "./dist/vendor/type-compiler/*.js"
      },
      "require": {
        "types": "./dist/vendor/type-compiler/*.d.cts",
        "default": "./dist/vendor/type-compiler/*.cjs"
      },
      "default": {
        "types": "./dist/vendor/type-compiler/*.d.ts",
        "default": "./dist/vendor/type-compiler/*.js"
      }
    },
    "./schemas/*": {
      "import": {
        "types": "./schemas/*.d.ts",
        "default": "./schemas/*.js"
      },
      "require": {
        "types": "./schemas/*.d.cts",
        "default": "./schemas/*.cjs"
      },
      "default": {
        "types": "./schemas/*.d.ts",
        "default": "./schemas/*.js"
      }
    }
  },
  "typings": "dist/index.d.ts",
  "files": ["dist/**/*", "bin/*", "schemas/*"],
  "keywords": ["deepkit", "powerlines", "storm-software"],
  "dependencies": {
    "@storm-software/config-tools": "catalog:",
    "@stryke/capnp": "catalog:",
    "@stryke/fs": "catalog:",
    "@stryke/path": "catalog:",
    "@stryke/type-checks": "catalog:",
    "@stryke/types": "catalog:",
    "chalk": "catalog:",
    "defu": "catalog:",
    "jiti": "catalog:",
    "powerlines": "0.10.1",
    "typescript": "^5.9.3",
    "unplugin": "catalog:"
  },
  "devDependencies": {
    "@deepkit/core": "catalog:",
    "@deepkit/type": "catalog:",
    "@deepkit/type-compiler": "catalog:",
    "@deepkit/type-spec": "catalog:",
    "@powerlines/nx": "0.8.2",
    "@types/node": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
