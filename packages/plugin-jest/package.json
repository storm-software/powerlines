  "contributors": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Apache-2.0",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./types": {
      "require": "./dist/types/index.cjs",
      "import": "./dist/types/index.mjs"
    },
    "./types/plugin": {
      "require": "./dist/types/plugin.cjs",
      "import": "./dist/types/plugin.mjs"
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/index.d.mts",
  "files": ["dist/**/*"],
  "keywords": ["jest", "powerlines", "storm-software", "powerlines-plugin"],
  "dependencies": {
    "@jest/reporters": "catalog:",
    "@jest/test-result": "catalog:",
    "@stryke/fs": "catalog:",
    "@stryke/path": "catalog:",
    "chalk": "catalog:",
    "defu": "catalog:",
    "jest": "catalog:",
    "jest-config": "catalog:",
    "jest-resolve": "catalog:",
    "jest-util": "catalog:",
    "powerlines": "workspace:*"
  },
  "devDependencies": {
    "@jest/types": "catalog:",
    "@powerlines/plugin-plugin": "workspace:*",
    "@types/jest": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/index.d.cts"
}
