      "@powerlines/plugin-rspack/*": ["packages/plugin-rspack/src/*"],
      "@powerlines/plugin-satori": ["packages/plugin-satori/src/index.ts"],
      "@powerlines/plugin-satori/*": ["packages/plugin-satori/src/*"],
      "@powerlines/plugin-image-compression": [
        "packages/plugin-image-compression/src/index.ts"
      ],
      "@powerlines/plugin-image-compression/*": [
        "packages/plugin-image-compression/src/*"
      ],
      "@powerlines/plugin-style-dictionary": [
        "packages/plugin-style-dictionary/src/index.ts"
      ],
      "@powerlines/plugin-style-dictionary/*": [
        "packages/plugin-style-dictionary/src/*"
