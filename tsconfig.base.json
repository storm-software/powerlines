{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@storm-software/tsconfig/core.json",
  "display": "Powerlines - Monorepo",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@powerlines/alloy": ["packages/alloy/src/index.ts"],
      "@powerlines/alloy/*": ["packages/alloy/src/*"],
      "@powerlines/deepkit": ["packages/deepkit/src/index.ts"],
      "@powerlines/deepkit/*": ["packages/deepkit/src/*"],
      "@powerlines/nx": ["packages/nx/index.ts"],
      "@powerlines/nx/*": ["packages/nx/src/*"],
      "@powerlines/plugin-asyncapi": ["packages/plugin-asyncapi/src/index.ts"],
      "@powerlines/plugin-asyncapi/*": ["packages/plugin-asyncapi/src/*"],
      "@powerlines/plugin-babel": ["packages/plugin-babel/src/index.ts"],
      "@powerlines/plugin-babel/*": ["packages/plugin-babel/src/*"],
      "@powerlines/plugin-biome": ["packages/plugin-biome/src/index.ts"],
      "@powerlines/plugin-biome/*": ["packages/plugin-biome/src/*"],
      "@powerlines/plugin-capnp": ["packages/plugin-capnp/src/index.ts"],
      "@powerlines/plugin-capnp/*": ["packages/plugin-capnp/src/*"],
      "@powerlines/plugin-crypto": ["packages/plugin-crypto/src/index.ts"],
      "@powerlines/plugin-crypto/*": ["packages/plugin-crypto/src/*"],
      "@powerlines/plugin-date": ["packages/plugin-date/src/index.ts"],
      "@powerlines/plugin-date/*": ["packages/plugin-date/src/*"],
      "@powerlines/plugin-deepkit": ["packages/plugin-deepkit/src/index.ts"],
      "@powerlines/plugin-deepkit/*": ["packages/plugin-deepkit/src/*"],
      "@powerlines/plugin-env": ["packages/plugin-env/src/index.ts"],
      "@powerlines/plugin-env/*": ["packages/plugin-env/src/*"],
      "@powerlines/plugin-esbuild": ["packages/plugin-esbuild/src/index.ts"],
      "@powerlines/plugin-esbuild/*": ["packages/plugin-esbuild/src/*"],
      "@powerlines/plugin-eslint": ["packages/plugin-eslint/src/index.ts"],
      "@powerlines/plugin-eslint/*": ["packages/plugin-eslint/src/*"],
      "@powerlines/plugin-id": ["packages/plugin-id/src/index.ts"],
      "@powerlines/plugin-id/*": ["packages/plugin-id/src/*"],
      "@powerlines/plugin-graphql": ["packages/plugin-graphql/src/index.ts"],
      "@powerlines/plugin-graphql/*": ["packages/plugin-graphql/src/*"],
      "@powerlines/plugin-jest": ["packages/plugin-jest/src/index.ts"],
      "@powerlines/plugin-jest/*": ["packages/plugin-jest/src/*"],
      "@powerlines/plugin-openapi": ["packages/plugin-openapi/src/index.ts"],
      "@powerlines/plugin-openapi/*": ["packages/plugin-openapi/src/*"],
      "@powerlines/plugin-oxc-transform": [
        "packages/plugin-oxc-transform/src/index.ts"
      ],
      "@powerlines/plugin-oxc-transform/*": [
        "packages/plugin-oxc-transform/src/*"
      ],
      "@powerlines/plugin-oxlint": ["packages/plugin-oxlint/src/index.ts"],
      "@powerlines/plugin-oxlint/*": ["packages/plugin-oxlint/src/*"],
      "@powerlines/plugin-plugin": ["packages/plugin-plugin/src/index.ts"],
      "@powerlines/plugin-plugin/*": ["packages/plugin-plugin/src/*"],
      "@powerlines/plugin-rolldown": ["packages/plugin-rolldown/src/index.ts"],
      "@powerlines/plugin-rolldown/*": ["packages/plugin-rolldown/src/*"],
      "@powerlines/plugin-rollup": ["packages/plugin-rollup/src/index.ts"],
      "@powerlines/plugin-rollup/*": ["packages/plugin-rollup/src/*"],
      "@powerlines/plugin-rspack": ["packages/plugin-rspack/src/index.ts"],
      "@powerlines/plugin-rspack/*": ["packages/plugin-rspack/src/*"],
      "@powerlines/plugin-style-dictionary": [
        "packages/plugin-style-dictionary/src/index.ts"
      ],
      "@powerlines/plugin-style-dictionary/*": [
        "packages/plugin-style-dictionary/src/*"
      ],
      "@powerlines/plugin-stylelint": [
        "packages/plugin-stylelint/src/index.ts"
      ],
      "@powerlines/plugin-stylelint/*": ["packages/plugin-stylelint/src/*"],
      "@powerlines/plugin-swc": ["packages/plugin-swc/src/index.ts"],
      "@powerlines/plugin-swc/*": ["packages/plugin-swc/src/*"],
      "@powerlines/plugin-tsc": ["packages/plugin-tsc/src/index.ts"],
      "@powerlines/plugin-tsc/*": ["packages/plugin-tsc/src/*"],
      "@powerlines/plugin-tsup": ["packages/plugin-tsup/src/index.ts"],
      "@powerlines/plugin-tsup/*": ["packages/plugin-tsup/src/*"],
      "@powerlines/plugin-typedoc": ["packages/plugin-typedoc/src/index.ts"],
      "@powerlines/plugin-typedoc/*": ["packages/plugin-typedoc/src/*"],
      "@powerlines/plugin-unbuild": ["packages/plugin-unbuild/src/index.ts"],
      "@powerlines/plugin-unbuild/*": ["packages/plugin-unbuild/src/*"],
      "@powerlines/plugin-vite": ["packages/plugin-vite/src/index.ts"],
      "@powerlines/plugin-vite/*": ["packages/plugin-vite/src/*"],
      "@powerlines/plugin-webpack": ["packages/plugin-webpack/src/index.ts"],
      "@powerlines/plugin-webpack/*": ["packages/plugin-webpack/src/*"],
      "@powerlines/tools-config/*": ["tools/config/*"],
      "@powerlines/tools-scripts": ["tools/scripts/src/index.ts"],
      "@powerlines/tools-scripts/*": ["tools/scripts/src/*"],
      "@powerlines/tsconfig/*": ["packages/tsconfig/src/*"],
      "powerlines": ["packages/powerlines/src/index.ts"],
      "powerlines/*": ["packages/powerlines/src/*"]
    },
    "verbatimModuleSyntax": false
  }
}
