      "@powerlines/plugin-untyped": ["packages/plugin-untyped/src/index.ts"],
      "@powerlines/plugin-untyped/*": ["packages/plugin-untyped/src/*"],
      "@powerlines/plugin-vite": ["packages/plugin-vite/src/index.ts"],
      "@powerlines/plugin-vite/*": ["packages/plugin-vite/src/*"],
      "@powerlines/plugin-webpack": ["packages/plugin-webpack/src/index.ts"],
      "@powerlines/plugin-webpack/*": ["packages/plugin-webpack/src/*"],
      "@powerlines/tools-config/*": ["tools/config/*"],
      "@powerlines/tools-scripts": ["tools/scripts/src/index.ts"],
      "@powerlines/tools-scripts/*": ["tools/scripts/src/*"],
      "@powerlines/tools-nx": ["tools/nx/src/index.ts"],
      "@powerlines/tools-nx/*": ["tools/nx/src/*"],
      "@powerlines/tsconfig/*": ["packages/tsconfig/src/*"],
      "powerlines": ["packages/powerlines/src/index.ts"],
      "powerlines/*": ["packages/powerlines/src/*"]
    },
    "verbatimModuleSyntax": false
  }
}
